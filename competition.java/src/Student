import java.io.BufferedReader;
import java.io.FileReader;
import java.io.IOException;

public class Student {
    private String id;
    private String name;
    private String address;
    private String telephone;
    private String status;

    public Student(String id, String name, String address, String telephone, String status) {
        this.id = id;
        this.name = name;
        this.address = address;
        this.telephone = telephone;
        this.status = status;
    }

    public static Student fromFile(String filename) throws IOException {
        BufferedReader br = new BufferedReader(new FileReader(filename));
        String id = br.readLine();
        String name = br.readLine();
        String address = br.readLine();
        String telephone = br.readLine();
        String status = br.readLine();
        br.close();
        return new Student(id, name, address, telephone, status);
    }

    public String getName() {
        return name;
    }

    public String getId() {
        return id;
    }

    public String getAddress() {
        return address;
    }

    public String getTelephone() {
        return telephone;
    }

    public String getStatus() {
        return status;
    }
}
